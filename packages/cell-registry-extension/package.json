{
  "name": "@e2xgrader/cell-registry-extension",
  "version": "0.1.0",
  "description": "Provides a registry for custom e2xgrader cells",
  "keywords": [
    "jupyter",
    "jupyterlab",
    "jupyterlab-extension"
  ],
  "homepage": "https://github.com/DigiKlausur/e2xgrader",
  "bugs": {
    "url": "https://github.com/DigiKlausur/e2xgrader/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Tim Metzler",
    "email": "tim.metzler@h-brs.de"
  },
  "files": [
    "lib/**/*.{d.ts,eot,gif,html,jpg,js,js.map,json,png,svg,woff2,ttf}",
    "src/**/*.{ts,tsx}"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "https://github.com/DigiKlausur/e2xgrader.git"
  },
  "scripts": {
    "build": "jlpm run build:lib && jlpm run build:labextension:dev",
    "build:prod": "jlpm clean && jlpm run build:lib && jlpm run build:labextension",
    "build:lib": "tsc -b",
    "build:labextension": "jupyter labextension build .",
    "build:labextension:dev": "jupyter labextension build --development True .",
    "clean": "run-p clean:lib clean:labextension",
    "clean:lib": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "clean:labextension": "rimraf ../../e2xgrader/static/labextensions/@e2xgrader/cell-registry-extension",
    "watch": "run-p watch:src watch:labextension",
    "watch:src": "tsc --watch --sourceMap",
    "watch:labextension": "jupyter labextension watch ."
  },
  "dependencies": {
    "@e2xgrader/cell-core": "^0.1.0",
    "@jupyterlab/application": "^4.2.5",
    "@lumino/signaling": "^2.1.3"
  },
  "devDependencies": {
    "@jupyterlab/builder": "^4.3.4",
    "npm-run-all": "^4.1.5",
    "rimraf": "^6.0.1",
    "typescript": "^5.7.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "extension": true,
    "outputDir": "../../e2xgrader/static/labextensions/@e2xgrader/cell-registry-extension"
  }
}

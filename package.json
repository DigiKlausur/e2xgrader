{
  "name": "@e2xgrader/root",
  "private": true,
  "workspaces": {
    "packages": [
      "packages/*"
    ]
  },
  "version": "0.3.0",
  "description": "An addon for nbgrader",
  "scripts": {
    "build": "lerna run build",
    "build:prod": "lerna run build:prod",
    "build:lib": "lerna run build:lib",
    "build:test": "lerna run build:test",
    "clean": "lerna run clean",
    "eslint": "eslint . --ext .ts,.tsx --fix",
    "eslint:check": "eslint . --ext .ts,.tsx",
    "eslint:files": "eslint --fix",
    "lint": "lerna run lint",
    "lint:check": "lerna run lint:check",
    "prettier": "lerna run prettier",
    "prettier:files": "prettier --write",
    "prettier:base": "lerna run prettier:base",
    "prettier:check": "lerna run prettier:check",
    "stylelint": "lerna run stylelint",
    "stylelint:check": "lerna run stylelint:check",
    "test": "jest --coverage",
    "watch": "run-p watch:labextension",
    "watch:labextension": "lerna exec --stream --scope @e2xgrader/labextension --scope @e2xgrader/cell-extension -- jlpm watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DigiKlausur/e2xgrader.git"
  },
  "keywords": [
    "nbgrader",
    "e2xgrader",
    "teaching",
    "jupyter",
    "notebook"
  ],
  "author": "Tim Metzler",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.2.0",
    "@types/json-schema": "^7.0.11",
    "@types/react": "^18.0.26",
    "@types/react-addons-linked-state-mixin": "^0.14.22",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "css-loader": "^6.7.1",
    "eslint": "^8.36.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.2.0",
    "lerna": "^7.4.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.1",
    "source-map-loader": "^1.0.2",
    "style-loader": "^3.3.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recommended": "^13.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-csstree-validator": "^3.0.0",
    "stylelint-prettier": "^4.0.0",
    "typescript": "~5.0.2",
    "yjs": "^13.5.0"
  },
  "bugs": {
    "url": "https://github.com/DigiKlausur/e2xgrader/issues"
  },
  "homepage": "https://github.com/DigiKlausur/e2xgrader#readme"
}
